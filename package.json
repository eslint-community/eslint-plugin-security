{
  "name": "eslint-plugin-security",
  "version": "3.0.1",
  "description": "Security rules for eslint",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "bun": {
        "types": "./dist/index.d.ts",
        "default": "./index.ts"
      },
      "module-sync": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "module": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run clean && tsdown --config-loader='unrun' --config=tsdown.config.mts",
    "changelog": "changelog eslint-plugin-security all > CHANGELOG.md",
    "check-exports": "attw --pack .",
    "clean": "rimraf dist/ .rolldown/",
    "cont-int": "npm test && npm run lint",
    "format": "prettier --write .",
    "lint": "npm-run-all \"lint:*\"",
    "lint:docs": "markdownlint \"**/*.md\"",
    "lint:eslint-docs": "npm run update:eslint-docs -- --check",
    "lint:js": "eslint --config=\"eslint.config.ts\" .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "prepack": "npm run build",
    "release": "npx semantic-release",
    "test": "mocha --node-option=\"experimental-strip-types\" \"test/**/*.test.ts\"",
    "typecheck": "tsc -p tsconfig.json",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eslint-community/eslint-plugin-security.git"
  },
  "keywords": [
    "eslint",
    "security",
    "nodesecurity"
  ],
  "author": "Node Security Project",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/eslint-community/eslint-plugin-security/issues"
  },
  "homepage": "https://github.com/eslint-community/eslint-plugin-security#readme",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx,mts,mjs,cts,cjs}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.md": "prettier --write",
    "*.yml": "prettier --write"
  },
  "dependencies": {
    "safe-regex": "^2.1.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@eslint/core": "^1.0.0",
    "@eslint/js": "^9.39.1",
    "@types/estree": "^1.0.8",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.1",
    "@types/safe-regex": "^1.1.6",
    "changelog": "1.4.2",
    "eslint": "^9.39.1",
    "eslint-config-nodesecurity": "^1.3.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-doc-generator": "^2.3.0",
    "eslint-plugin-eslint-plugin": "^7.2.0",
    "jiti": "^2.6.1",
    "lint-staged": "^12.3.7",
    "markdownlint-cli": "^0.32.2",
    "mocha": "^11.7.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.3",
    "rimraf": "^6.1.2",
    "rolldown": "^1.0.0-beta.52",
    "semantic-release": "^19.0.2",
    "tsdown": "^0.17.0-beta.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "funding": "https://opencollective.com/eslint"
}
